{
  "expo": {
    "name": "RentAHouse",
    "displayName": "Rent A House",
    "slug": "Rent-A-House",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "rentahouse",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,

    "splash": {
      "image": "./assets/images/splash.png",
      "resizeMode": "cover",
      "backgroundColor": "#ffffff"
    },

    "ios": {
      "supportsTablet": true
    },

    "android": {
      "adaptiveIcon": {
        "backgroundColor": "#ffffff",
        "backgroundImage": "./assets/images/adaptive-icon.png"
      },
      "edgeToEdgeEnabled": true,
      "predictiveBackGestureEnabled": false,
      "package": "com.anonymous.RentAHouse"
    },

    "web": {
      "output": "static",
      "favicon": "./assets/images/adaptive-icon.png"
    },

    "plugins": [
      "expo-router",
      "expo-splash-screen",
      "expo-web-browser"
    ],

    "experiments": {
      "typedRoutes": true,
      "reactCompiler": true
    },

    "extra": {
      "router": {},
      "eas": {
        "projectId": "b5f1e9d5-5e73-4bdb-8540-147020f287ba"
      }
    },

    "owner": "yungbrat"
  }
}
